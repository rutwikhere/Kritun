{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "PROJECT_ID",
          "appId": "ANDROID_APP_ID",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "PROJECT_ID",
          "configurations": {
            "android": "ANDROID_APP_ID",
            "ios": "IOS_APP_ID",
            "macos": "MACOS_APP_ID",
            "web": "WEB_APP_ID",
            "windows": "WINDOWS_APP_ID"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ]
}
